{
  "name": "Premi",
  "repository": {
    "type": "git",
    "url": "https://github.com/PetrucciMauro/source"
  },
  "description": "Testing node.js",
  "author": "LateButSafe",
  "version": "0.0.1",
  "devDependencies": {
    "mocha": "latest",
    "istanbul":"latest",
    "coveralls":"latest"
  },
  "scripts": {
    "test": "mocha",
    "coverage":"istanbul cover ./usr/local/lib/node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "coveralls":"istanbul cover ./usr/local/lib/node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  }
}



